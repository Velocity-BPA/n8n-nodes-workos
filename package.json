{
  "name": "n8n-nodes-workos",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for WorkOS providing 12 resources and 70+ operations for SSO, Directory Sync, user management, organizations, and audit logging. Includes webhook triggers for real-time event automation.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-workos.git"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "workos",
    "sso",
    "saml",
    "oidc",
    "directory-sync",
    "scim",
    "authentication",
    "enterprise",
    "identity",
    "user-management"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WorkOsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WorkOS/WorkOS.node.js",
      "dist/nodes/WorkOS/WorkOSTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.30.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
